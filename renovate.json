{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", "npm:unpublishSafe", "schedule:earlyMondays"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchPackageNames": ["aws-cdk-lib", "constructs"],
      "enabled": false
    }
  ]
}
